      "source": [
        "def gcd(a, b):\n",
        "    while(b != 0):\n",
        "        n = a%b\n",
        "        a = b\n",
        "        b = n\n",
        "    return a\n",
        "\n",
        "n = int(input())\n",
        "li = list(map(int, input().split()))\n",
        "for i in range(1, n):\n",
        "    g = gcd(li[0], li[i])\n",
        "    print('{0}/{1}'.format(li[0]//g, li[i]//g))"
      ]
    }
  ]
}