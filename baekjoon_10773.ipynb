        "rep = int(input())\n",
        "ls = []\n",
        "sum = 0\n",
        "for _ in range(rep):\n",
        "    inp = int(input())\n",
        "    if inp == 0:\n",
        "        if len(ls) != 0:\n",
        "            x = ls.pop()\n",
        "            sum -= x\n",
        "    else:\n",
        "        ls.append(inp)\n",
        "        sum += inp\n",
        "print(sum)"
      ]
    }
