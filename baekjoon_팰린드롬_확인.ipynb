      "source": [
        "x = input()\n",
        "y = 0\n",
        "t = 0\n",
        "for i in range(len(x)-1, -1, -1):\n",
        "    if x[i] == x[t]:\n",
        "        y = 1\n",
        "    else:\n",
        "        y = 0\n",
        "        break\n",
        "    t += 1\n",
        "\n",
        "print(int(y))"
      ]
