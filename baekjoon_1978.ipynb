      "source": [
        "su = int(input())\n",
        "a = list(map(int, input().split()))\n",
        "\n",
        "a = a[0:su]\n",
        "count = 0\n",
        "cnt = 0\n",
        "for i in a :\n",
        "    for j in range(1, i + 1):\n",
        "        if i % j == 0:\n",
        "            cnt += 1\n",
        "\n",
        "    if cnt == 2:\n",
        "        count += 1\n",
        "    cnt = 0\n",
        "\n",
        "print(count)"
      ]
