        "a,b = map(int, input().split())\n",
        "\n",
        "def gcd(a,b):\n",
        "    while b:\n",
        "        mod = b\n",
        "        b= a%b\n",
        "        a = mod\n",
        "    return a\n",
        "print(a*b//gcd(a,b))"
