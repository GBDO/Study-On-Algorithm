      "source": [
        "import sys\n",
        "num = sys.stdin.readline()\n",
        "ls = [0] * len(num)\n",
        "for i in range(len(num)):\n",
        "    ls[i] = num[i]\n",
        "ls.sort(reverse=True)\n",
        "result = ''\n",
        "for k in range(len(num)):\n",
        "    result += str(ls[k])\n",
        "print(result)"
      ]
