      "source": [
        "from math import gcd\n",
        "\n",
        "def solution(string):\n",
        "    num1, num2 = map(int, string.split(\":\"))\n",
        "\n",
        "    gcd_num = gcd(num1, num2)\n",
        "\n",
        "    return f\"{num1 // gcd_num}:{num2 // gcd_num}\"\n",
        "\n",
        "string = input()\n",
        "print(solution(string))"
      ]
    }
  ]
