        "ls = []\n",
        "sum = 0\n",
        "result = []\n",
        "for k in range(8):\n",
        "    x = int(input())\n",
        "    ls.append([x,k+1])\n",
        "ls.sort(key = lambda x : -x[0])\n",
        "for i in range(5):\n",
        "    sum += ls[i][0]\n",
        "    result.append(ls[i][1])\n",
        "print(sum)\n",
        "result.sort()\n",
        "for i in range(5):\n",
        "    print(result[i],end=' ')"
